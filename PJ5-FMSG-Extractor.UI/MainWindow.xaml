<Window x:Class="PJ5_FMSG_Extractor.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PJ5_FMSG_Extractor.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="120" Width="340">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Name="BtnExtract" Content="Extract" Width="150" HorizontalAlignment="Center" VerticalAlignment="Center" Height="62" Click="BtnExtract_Click"/>
        <Button Grid.Column="1" Name="BtnInsert" Content="Insert" Width="150" HorizontalAlignment="Center" VerticalAlignment="Center" Height="62" Click="BtnInsert_Click"/>

    </Grid>
</Window>
